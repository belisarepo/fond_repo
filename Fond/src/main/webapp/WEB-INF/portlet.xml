		</supports>
		<portlet-info>
			<title>Заявка</title>
			<short-title>Заявка</short-title>
			<keywords>Заявка</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
